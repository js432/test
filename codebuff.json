{
  "description": "Template configuration for this project. See https://www.codebuff.com/config for all options.",
  "startupProcesses": [
    {
      "name": "dev",
      "command": "npm run dev",
      "enabled": true,
      "stdoutFile": "logs/dev.log"
    }
  ],
  "fileChangeHooks": [
    {
      "name": "lint",
      "command": "npm run lint -- --fix",
      "enabled": true
    },
    {
      "name": "typecheck",
      "command": "npm run typecheck",
      "enabled": true
    }
  ]
}
